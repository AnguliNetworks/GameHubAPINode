{
  "extends": "tslint-config-airbnb",
  "rules": {
    "ter-indent": [
      "error",
      2
    ],
    "no-increment-decrement": [
      "never"
    ],
    "max-line-length": [
      120
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  }
}
